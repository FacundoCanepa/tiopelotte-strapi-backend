{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "text"
    },
    "img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "decimal"
    },
    "unidadMedida": {
      "type": "enumeration",
      "enum": [
        "kg",
        "planchas",
        "unidad"
      ]
    },
    "taste": {
      "type": "enumeration",
      "enum": [
        "fideos",
        "sorrentinos",
        "ravioles",
        "especiales",
        "Ã±oquis",
        "canelones",
        "filetto",
        "queso",
        "flan",
        "chocotorta",
        "pastafrola"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "descriptionCorta": {
      "type": "string"
    },
    "isOffer": {
      "type": "boolean"
    },
    "img_carousel": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "stock": {
      "type": "decimal"
    },
    "ingredientes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ingrediente.ingrediente",
      "inversedBy": "products"
    },
    "stockUpdatedAt": {
      "type": "datetime"
    },
    "tiempoEstimado": {
      "type": "string"
    },
    "porciones": {
      "type": "string"
    },
    "recetas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::receta.receta",
      "mappedBy": "products"
    }
  }
}
